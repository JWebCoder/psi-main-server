{
  "name": "psi-main-server",
  "version": "0.0.1",
  "description": "psi functions main server",
  "main": "bin/www",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/*.js",
    "dev": "DEBUG=raspsi:* babel-node ./bin_src/www",
    "clean": "rm -rf lib bin && mkdir lib && mkdir bin",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src -d lib --extensions \".ts,.tsx\" --source-maps inline && babel bin_src/www.ts -o bin/www --extensions \".ts,.tsx\" --source-maps inline && node buildSteps/index.js",
    "start": "NODE_ENV=production node ./bin/www"
  },
  "bin": {
    "www": "./bin/www"
  },
  "keywords": [
    "raspberry",
    "psi",
    "function",
    "distribution",
    "serverless"
  ],
  "author": "Jo√£o Moura",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.31",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/shortid": "0.0.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "replace-in-file": "^3.4.3",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "shortid": "^2.2.14"
  }
}
